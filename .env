# .env

# Ports (Optional: change if default ports 3000, 5173, or 3307 are busy on your host)
# BACKEND_PORT=3000
# FRONTEND_PORT=5173
# DB_HOST_PORT=3307

# Database Credentials (CHANGE THESE!)
MYSQL_ROOT_PASSWORD=your_strong_root_password
DB_PASSWORD=your_strong_app_password
DB_USER=sudoku_user
DB_NAME=sudoku

# Backend DB Sync (set to false for production-like behavior)
DB_SYNC=true # Set to true for initial dev setup, false for production